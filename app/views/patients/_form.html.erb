<h2><%= form_for (@patient) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>
  <%= f.label :password %>
  <%= f.password_field :password %>
  <br>
  <%= f.label :age %>
  <%= f.text_field :age %>
  <br>
  <%= f.label :height %>
  <%= f.text_field :height %>
  <br>
  <%= f.label :date_of_birth, "Date of Birth:" %>
  <%= f.date_field :date_of_birth %>
  <br>
  <%= f.submit %>
  <% end %>
  <br>
