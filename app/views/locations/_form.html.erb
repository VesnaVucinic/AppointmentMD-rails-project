<h2><%= form_for (@location) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :street %>
  <%= f.password_field :street %>
  <br>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <br>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <br>
  <%= f.label :zipcode %>
  <%= f.text_field :zipcode %>
  <br>
  <%= f.label :hours_of_operation %>
  <%= f.time_field :hours_of_operation %>
  <br>
  <%= f.label :physician_id %>
  <%= f.collection_select :physician_id, Physician.includes(:location).all.keep_if {|physician|physician.location.blank?}, :id, :name,{:include_blank => 'None'} %>
  <br>
  <%= f.submit %>
  <% end %>
  <br>
