<h2><%= form_for (@location) do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :street %>
  <%= f.text_field :street %>
  <br>
  <%= f.label :city %>
  <%= f.text_field :city %>
  <br>
  <%= f.label :state %>
  <%= f.text_field :state %>
  <br>
  <%= f.label :zipcode %>
  <%= f.text_field :zipcode %>
  <br>
  <%= f.label :hours_of_operation %>
  <%= f.time_field :hours_of_operation %>
  <br>
  <%= f.submit %>
  <% end %>
  <br>
